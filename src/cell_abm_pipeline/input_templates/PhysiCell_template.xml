<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>0</x_min>
		<x_max>245</x_max>
		<y_min>0</y_min>
		<y_max>165</y_max>
		<z_min>0</z_min>
		<z_max>100</z_max>
		<dx>5</dx>
		<dy>5</dy>
		<dz>5</dz>
		<use_2D>false</use_2D>
	</domain>
	
	<overall>
        <!-- 5 days * 24 h * 60 min -->
		<max_time units="min">7200</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">{{dt_diffusion}}</dt_diffusion>
		<dt_mechanics units="min">{{dt_mechanics}}</dt_mechanics>
		<dt_phenotype units="min">{{dt_phenotype}}</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder>

		<full_data>
			<interval units="min">36</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<enable>false</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
		<virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>				
	</options>
	
	<cell_definitions>

		<cell_definition name="substrate" ID="0">
			<phenotype>

				<cycle code="8" name="Quiescent model"></cycle>					

				<volume>  
					<total units="micron^3">{{subcell_volume}}</total>
					<fluid_fraction units="dimensionless">0.75</fluid_fraction>
					<nuclear units="micron^3">0.</nuclear>
					
					<fluid_change_rate units="1/min">0.</fluid_change_rate>
					<cytoplasmic_biomass_change_rate units="1/min">0.</cytoplasmic_biomass_change_rate>
					<nuclear_biomass_change_rate units="1/min">0.</nuclear_biomass_change_rate>
					
					<calcified_fraction units="dimensionless">0</calcified_fraction>
					<calcification_rate units="1/min">0</calcification_rate>
					
					<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
				</volume> 				
				
				<mechanics> 
					<cell_cell_adhesion_strength units="micron/min">{{substrate_adhesion}}</cell_cell_adhesion_strength>
					<cell_cell_repulsion_strength units="micron/min">{{substrate_repulsion}}</cell_cell_repulsion_strength>
					<relative_maximum_adhesion_distance units="dimensionless">{{substrate_adhesion_distance}}</relative_maximum_adhesion_distance>
					
					<options>
						<set_relative_equilibrium_distance enabled="true" units="dimensionless">1.75</set_relative_equilibrium_distance>
						<set_absolute_equilibrium_distance enabled="false" units="micron">10</set_absolute_equilibrium_distance>
					</options>
				</mechanics>

			</phenotype>
		</cell_definition>

		<cell_definition name="subcell" ID="1">
			<phenotype>

				<cycle code="7" name="Cycling-Quiescent model"></cycle>

				<death>  
					<model code="100" name="apoptosis"> 
						<rate units="1/min">{{subcell_death_rate}}</rate>
					</model>
				</death>			

				<volume>  
					<total units="micron^3">{{subcell_volume}}</total>
					<fluid_fraction units="dimensionless">0.75</fluid_fraction>
					<nuclear units="micron^3">0.</nuclear>
					
					<fluid_change_rate units="1/min">{{subcell_volume_growth_rate}}</fluid_change_rate>
					<cytoplasmic_biomass_change_rate units="1/min">{{subcell_volume_growth_rate_cytoplasmic}}</cytoplasmic_biomass_change_rate>
					<nuclear_biomass_change_rate units="1/min">0.</nuclear_biomass_change_rate>
					
					<calcified_fraction units="dimensionless">0</calcified_fraction>
					<calcification_rate units="1/min">0</calcification_rate>
					
					<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
				</volume> 				
				
				<mechanics> 
					<cell_cell_adhesion_strength units="micron/min">{{subcell_adhesion}}</cell_cell_adhesion_strength>
					<cell_cell_repulsion_strength units="micron/min">{{subcell_repulsion}}</cell_cell_repulsion_strength>
					<relative_maximum_adhesion_distance units="dimensionless">{{subcell_adhesion_distance}}</relative_maximum_adhesion_distance>
					
					<options>
						<set_relative_equilibrium_distance enabled="true" units="dimensionless">2.</set_relative_equilibrium_distance>
						<set_absolute_equilibrium_distance enabled="false" units="micron">10</set_absolute_equilibrium_distance>
					</options>
				</mechanics>
				
				<motility>  
					<speed units="micron/min">{{subcell_speed}}</speed>
					<persistence_time units="min">1</persistence_time>
					<migration_bias units="dimensionless">0.1</migration_bias>
					
					<options>
						<enabled>true</enabled>
						<use_2D>false</use_2D>
					</options>
				</motility>

			</phenotype>
		</cell_definition>

		<cell_definition name="Cell" ID="2">
			<phenotype>

				<cycle code="6" name="Flow cytometry model (separated)">  
					<!-- phases are: G0/G1 , S, G2, M -->
					<phase_transition_rates units="1/min"> 
						<rate start_index="0" end_index="1" fixed_duration="false">0</rate>
						<rate start_index="1" end_index="2" fixed_duration="true">0.00208333</rate>
						<rate start_index="2" end_index="3" fixed_duration="true">0.00416667</rate>
						<rate start_index="3" end_index="0" fixed_duration="true">0.0166667</rate>
					</phase_transition_rates>
				</cycle>

				<volume>  
					<total units="micron^3">3300.0</total>
					<fluid_fraction units="dimensionless">0.</fluid_fraction>
					<nuclear units="micron^3">0.</nuclear>
					
					<fluid_change_rate units="1/min">0.</fluid_change_rate>
					<cytoplasmic_biomass_change_rate units="1/min">0.</cytoplasmic_biomass_change_rate>
					<nuclear_biomass_change_rate units="1/min">0.</nuclear_biomass_change_rate>
					
					<calcified_fraction units="dimensionless">0</calcified_fraction>
					<calcification_rate units="1/min">0</calcification_rate>
					
					<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
				</volume>

			</phenotype>
		</cell_definition>
	</cell_definitions>
	
	<initial_conditions>
		<cell_positions type="csv" enabled="true">
			<folder>./config</folder>
			<filename>{{init_conditions_file_name}}</filename>
			<owner_cell_definition_name>Cell</owner_cell_definition_name>
			<member_cell_definition_name>subcell</member_cell_definition_name>
			<subcell_radius_microns>{{subcell_radius}}</subcell_radius_microns>
            <position_offset_x type="double" units="micron" description="offset from CSV positions for X dimension">-368.</position_offset_x>
            <position_offset_y type="double" units="micron" description="offset from CSV positions for Y dimension">-248.</position_offset_y>
            <position_offset_z type="double" units="micron" description="offset from CSV positions for Y dimension">25.</position_offset_z>
		</cell_positions>
	</initial_conditions>		
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed>
		<relative_heterotypic_adhesion type="double" description="relative strength of adhesion to agents of other types" units="dimensionless">{{relative_heterotypic_adhesion}}</relative_heterotypic_adhesion>
		<owner_death_volume type="double" units="micron^3" description="threshold volume for owner cell death">{{owner_death_volume}}</owner_death_volume>
		<owner_motility_bias type="double" units="dimensionless" description="normalized bias for how random vs persistent motion is">{{owner_motility_bias}}</owner_motility_bias>
		<owner_persistence_time type="double" units="min" description="duration for persistent motion">{{owner_motility_switch_time}}</owner_persistence_time>
		<owner_basal_apoptosis_rate type="double" units="1/min" description="rate of basal cell death">{{owner_basal_apoptosis_rate}}</owner_basal_apoptosis_rate>
		<subcell_transition_multiplier type="double" units="dimensionless" description="multiplying factor for rate of quiescent to cycling">{{subcell_transition_multiplier}}</subcell_transition_multiplier>
	</user_parameters>
	
</PhysiCell_settings>
